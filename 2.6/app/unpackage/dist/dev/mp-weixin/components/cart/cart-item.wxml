<view><view class="cart-list"><block wx:for="{{cartList}}" wx:for-item="item" wx:for-index="index" wx:key="id"><block><view class="cart-item"><checkbox checked="{{item.checked}}" color="#FF3333" data-event-opts="{{[['tap',[['check',['item',index]]]]]}}" bindtap="__e"></checkbox><view class="image-wrapper"><image src="{{item.goods_image}}" mode="aspectFill"></image></view><view class="item-right"><text class="title">{{item.goods_name}}</text><text class="attr">{{item.goods_spec}}</text><view class="item-last"><text class="discount-price">{{"¥"+item.goods_discount}}</text><text class="goods-price">{{"¥"+item.goods_price}}</text><uni-number-box vue-id="{{'1-'+index}}" min="{{1}}" value="{{item.number}}" isMin="{{item.number===1}}" index="{{index}}" data-index="{{item.index}}" data-event-opts="{{[['^eventChange',[['numberChange']]]]}}" bind:eventChange="__e" bind:__l="__l"></uni-number-box></view></view></view></block></block></view><view class="action-section"><view class="total-box"><text class="price-title">合计：</text><text class="price-number">{{"¥"+totalPrice}}</text></view><view data-event-opts="{{[['tap',[['createOrder',['$event']]]]]}}" class="confirm-btn" bindtap="__e">去结算</view></view></view>