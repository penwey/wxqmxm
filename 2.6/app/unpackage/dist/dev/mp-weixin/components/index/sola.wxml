<view class="wrapper"><block wx:if="{{banner!==undefined}}"><view class="sola-head"><view class="sola-head-left"><text class="sola-topic">{{banner.title}}</text></view><view data-event-opts="{{[['tap',[['press',['$event']]]]]}}" class="sola-head-right" bindtap="__e"><text class="sola-link">{{banner.link}}</text><image class="sola-arrow" src="/static/arrow.png"></image></view></view></block><view class="sola-list"><block wx:for="{{list}}" wx:for-item="item" wx:for-index="index" wx:key="index"><view data-event-opts="{{[['tap',[['detail',['$event']]]]]}}" class="sola-product" style="{{'background-color:'+(item.dynamicallyColor)+';'}}" bindtap="__e"><image class="sola-image" src="{{item.uri}}"></image><text class="sola-title">{{item.title}}</text></view></block></view></view>